import java.lang.String;
import java.lang.System;

class GenericProgram
{


	<GT> void findSum(GT a,GT b)
	{

		System.out.println(a+" "+b);
	}



}


class GenericDemo
{


	public static void main(String args[])
	{

		GenericProgram gp=new GenericProgram();

		gp.findSum(3,8);
		gp.findSum(4.4,5.6);
		
	}

}
